<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://sun</uri>
    </include>
    
    <plugin name='buoyancy' filename='libBuoyancyPlugin.so'>
        <fluid_density>999.1026</fluid_density>
        <link name="base_link">
            <center_of_volume>-0.0239 0.114 -0.007</center_of_volume>
            <volume>0.00959081</volume>
            <neutrally_buoyant>0</neutrally_buoyant>
