    <plugin name='hydrodynamics_plugin' filename='libHydrodynamicsPlugin.so'>
      <link_name>base_link</link_name>
      <fluid_density>999.1026</fluid_density>
      <linear_drag_coefficients>10.0 10.0 10.0</linear_drag_coefficients>
      <angular_drag_coefficients>1.0 1.0 1.0</angular_drag_coefficients>
    </plugin>

  </world>
</sdf>